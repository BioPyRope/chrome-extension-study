{
	"manifest_version" : 2,
	"name" : "鐵人賽-ContentScript範例",
	"description" : "",
	"version" : "2.0",
    "page_action": {
        "default_title": "",
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    }, 	
	"content_scripts" : [
		{
			"matches" : ["*://www.google.com.tw/*"],
			"js" : ["jquery-3.1.1.min.js","content.js"],
			"css" : ["animate.css"]
		}
	],
    "background" : {
        "scripts" : ["event.js"],
        "persistent" : false
    },
	"permissions" : ["tabs"]
}